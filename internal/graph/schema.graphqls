# GraphQL schema example
#
# https://gqlgen.com/getting-started/

type Query {
  databases: [String]
  tables: [String]
  table(name: String): [TableInfo]
}

type TableInfo {
  field: String
  type: String
  null: String
  key: String
  default: Any
  extra: Any
}

type Mutation {
  createDatabase(name: String): CreateDatabaseResponse
}

type CreateDatabaseResponse {
  created: Int
}

scalar Any
